{
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "@unocss/reset": "^0.45.7",
    "@vueuse/components": "^9.1.0",
    "@vueuse/core": "^9.1.0",
    "@vueuse/motion": "2.0.0-beta.12",
    "core-js": "^3.24.1",
    "ip-regex": "^5.0.0",
    "leaflet": "^1.8.0",
    "markdown-it-shiki": "^0.5.1",
    "naive-ui": "^2.32.1",
    "p-limit": "^4.0.0",
    "pinia": "^2.0.18",
    "unocss": "^0.45.7",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3"
  },
  "devDependencies": {
    "@iconify-json/mdi": "^1.1.30",
    "@playwright/test": "^1.25.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/leaflet": "^1.7.11",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "playwright": "^1.25.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rollup": "^2.78.0",
    "rollup-plugin-visualizer": "^5.7.1",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.11.1",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.8",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-markdown": "^0.21.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/eslint-config-typescript/recommended",
      "@vue/eslint-config-prettier",
      "./.eslintrc-auto-import.json"
    ],
    "ignorePatterns": [
      "dist",
      "node_modules",
      ".git",
      "*.d.ts",
      "*.config.ts",
      "*.config.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2021
    },
    "root": true,
    "rules": {
      "@typescript-eslint/no-explicit-any": "off",
      "newline-per-chained-call": "off",
      "vue/html-indent": "off",
      "vue/max-attributes-per-line": "off",
      "vue/multi-word-component-names": "off",
      "vue/no-v-html": "off",
      "vue/singleline-html-element-content-newline": "off"
    }
  },
  "name": "geoip.pw",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "quoteProps": "as-needed",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "private": true,
  "scripts": {
    "build": "pnpm vite build",
    "lint": "pnpm eslint --ignore-path ../.gitignore --ext .js,.ts,.vue .",
    "preview": "pnpm run build && pnpm vite preview",
    "server": "pnpm vite",
    "test": "pnpm playwright test --repeat-each 3"
  },
  "version": "1.0.0"
}
